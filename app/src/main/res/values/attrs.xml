<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="AnalogClock">
        <attr name="custom_color" format="color"></attr>
    </declare-styleable>

    <declare-styleable name="TestView">
        <attr name="name" format="reference"/>
        <attr name="sex" format="reference"/>
        <attr name="age" format="integer"/>
        <attr name="weight">
            <flag name="fat" value="2"/>
            <flag name="mid" value="1"/>
            <flag name="thin" value="0"/>
        </attr>
        <attr name="adult" format="boolean"/>
        <attr name="textSize" format="dimension"/>
    </declare-styleable>

    <attr name="TestViewStyle" format="reference" />
</resources>
